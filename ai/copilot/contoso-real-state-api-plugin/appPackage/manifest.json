{
  "$schema": "https://developer.microsoft.com/json-schemas/teams/vDevPreview/MicrosoftTeams.schema.json",
  "manifestVersion": "devPreview",
  "version": "1.0.0",
  "id": "${{TEAMS_APP_ID}}",
  "developer": {
    "name": "Teams App, Inc.",
    "websiteUrl": "https://www.example.com",
    "privacyUrl": "https://www.example.com/privacy",
    "termsOfUseUrl": "https://www.example.com/termofuse"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "jm-realstate-${{APP_NAME_SUFFIX}}",
    "full": "Full name for jm-realstate"
  },
  "description": {
    "short": "JM Real Estate API",
    "full": "Finds up to date and detailed real estate properties for sale on the market"
  },
  "accentColor": "#FFFFFF",
  "composeExtensions": [
    {
      "composeExtensionType": "apiBased",
      "apiSpecificationFile": "apiSpecificationFile/openapi.yaml",
      "commands": [
        {
          "context": [
            "compose",
            "commandBox"
          ],
          "type": "query",
          "title": "Get a list of properties",
          "id": "getListings",
          "parameters": [
            {
              "name": "city",
              "title": "City",
              "description": "The city to search in",
              "inputType": "text"
            }
          ],
          "description": "Returns a list of properties, optionally filtered by providing the city, number of bedrooms, number of bathrooms and a list of a",
          "apiResponseRenderingTemplateFile": "responseTemplates/getListings.json"
        }
      ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": []
}